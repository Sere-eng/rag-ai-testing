# Copy this file to .env and fill in your keys
# cp .env.example .env

# Required: OpenRouter API key (get one at https://openrouter.ai/)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Required for embeddings: standard OpenAI API key (https://platform.openai.com/)
# If you want to use OpenRouter for embeddings too, you can set this to your OpenRouter key
OPENAI_API_KEY=your_openai_api_key_here

# Optional: LangSmith tracing (https://smith.langchain.com/)
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_ENDPOINT=https://eu.api.smith.langchain.com
